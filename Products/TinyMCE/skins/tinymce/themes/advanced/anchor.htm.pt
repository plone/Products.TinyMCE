<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" i18n:domain="tinymce">
<head>
	<title i18n:translate="advanced_dlg_anchor_title"></title>
	<script type="text/javascript" src="../../tiny_mce_popup.js"></script>
	<script type="text/javascript" src="js/anchor.js"></script>
</head>
<body>
	<div class="dialog-wrapper">
		<form onsubmit="AnchorDialog.update();return false;" action="#">
			<fieldset>
				<legend i18n:translate="advanced_dlg_anchor_title"></legend>
				<div class="field">
					<label for="anchorName" i18n:translate="advanced_dlg_anchor_name"></label>
					<span title="Required" class="fieldRequired">(Required)</span>
					<div class="widget"><input id="anchorName" name="anchorName" type="text" class="mceFocus" value="" size="30"/></div>
				</div>
			</fieldset>

			<div class="visualClear"><!-- --></div>

			<div class="mceActionPanel formControls">
				<span class="actionButtons">
					<input type="submit" id="insert" class="context" name="insert" i18n:attributes="value common_update" />
					<input type="button" id="cancel" class="context" name="cancel" i18n:attributes="value common_cancel" onclick="tinyMCEPopup.close();" />
				</span>
			</div>
		</form>
	</div>
</body>
</html>
