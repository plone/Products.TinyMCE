<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" i18n:domain="tinymce">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
	<title i18n:translate="paste_paste_word_desc"></title>
	<script type="text/javascript" src="../../tiny_mce_popup.js"></script>
	<script type="text/javascript" src="js/pasteword.js"></script>
</head>
<body>
	<div class="dialog-wrapper" id="content">
		<form name="source" onsubmit="return PasteWordDialog.insert();" action="#">
			<fieldset>
				<legend i18n:translate="paste_paste_word_desc"></legend>
				<div class="field">
					<label for="htmlSource" i18n:translate="paste_paste_word_desc"></label>
					<div class="formHelp" i18n:translate="paste_dlg_word_title"></div>
					<div class="widget">
						<div id="iframecontainer"></div>
					</div>
				</div>

			</fieldset>

			<div class="visualClear"><!-- --></div>

			<div class="mceActionPanel formControls">
				<span class="actionButtons">
					<input type="submit" id="insert" class="context" name="insert" i18n:attributes="value common_insert" />
					<input type="button" id="cancel" class="context" name="cancel" i18n:attributes="value common_cancel" onclick="tinyMCEPopup.close();" />
				</span>
			</div>
		</form>
	</div>
</body>
</html>
